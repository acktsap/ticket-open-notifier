<Configuration status="WARN">
    <Appenders>
        <Console name="CONSOLE" target="SYSTEM_OUT">
            <PatternLayout
                    pattern="%style{%d{YYYY-MM-dd HH:mm:ss.SSS}}{Cyan} %highlight{[%t] %-5level %c#L%L - %msg}{style=Logback}%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <Root level="INFO">
            <AppenderRef ref="CONSOLE"/>
        </Root>

        <Logger name="acktsap" level="DEBUG"/>
    </Loggers>
</Configuration>
